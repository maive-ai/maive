{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "dependsOn": ["^dev"],
      "persistent": false,
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "auth:schema:generate": {
      "cache": false,
      "interactive": true
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "format:fix": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "lint:fix": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "format:check": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**", "htmlcov/**", "coverage.xml"]
    },
    "changeset": {
      "cache": false
    },
    "version-packages": {
      "cache": false
    },
    "release": {
      "dependsOn": ["build", "test"],
      "cache": false
    }
  },
  "globalEnv": [],
  "globalPassThroughEnv": ["NODE_ENV", "CI", "npm_lifecycle_event"]
}
