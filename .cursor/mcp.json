{
    "mcpServers": {
      "@reapi/mcp-openapi": {
        "command": "npx",
        "args": ["-y", "@reapi/mcp-openapi@latest", "--dir", "./.cursor/external_docs/service_titan"],
        "env": {}
      },
      "assistant-ui": {
        "command": "npx",
        "args": ["-y", "@assistant-ui/mcp-docs-server"]
      },
      "shadcn": {
        "command": "npx",
        "args": ["shadcn@latest", "mcp"]
      },
      "vapi": {
        "command": "npx",
        "args": [
          "-y",
          "@vapi-ai/mcp-docs-server"
        ]
      },
      "chrome-devtools": {
        "command": "npx",
        "args": ["chrome-devtools-mcp@latest"]
      }
    }
  }